def digits(n):
    db = 0
    lsgood = []
    for i in range(0, n+1):
        i = str(i)
        for ch in i:
            if ch == '1' or ch == '2' or ch == '3' or ch == '5' or ch == '6' or ch == '8' or ch == '9':
                break
            lsgood.append(i)
            db += 1
    return (lsgood,db)

print(digits(60))
