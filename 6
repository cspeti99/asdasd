def digits(n):
    db = 0
    badnums = '1235689'
    lsgood = []
    for i in range(0, n + 1):
        szamok = [int(j) for j in str(i)]
        for j in szamok:
            if str(j) in badnums:
                break
        else:
            lsgood.append(i)
            db += 1
    return(lsgood, db)

print(digits(117))
